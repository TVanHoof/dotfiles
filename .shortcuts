alias f="ranger"   #mnemonic => files
alias q="exit"     #mnemonic => quit

function moveto {
    mv $2 $1
}

function copyto {
    cp $2 $1
}

# Home
alias gh="cd $HOME"
alias td="if [ -z $TMUX ]; then echo 'not in a tmux session'; else tmux new-window -c $HOME; fi"
alias md="moveto $HOME"
alias yh="copyto $HOME"

