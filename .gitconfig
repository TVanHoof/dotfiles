[core]
	editor = vim
[push]
	default = matching
[alias]
	co = checkout
	ci = commit
	br = branch
	st = status
  unstage = 'reset HEAD --'
[merge]
	tool = vim
	conflictstyle = diff3
[mergetool]
	prompt = false
[diff]
	tool = vim
[difftool]
	prompt = false
